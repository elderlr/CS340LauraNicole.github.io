<html>
<a href="/" class="list">Home</a>
<a href="/appointment" class="list">     Appointment Management</a>
<a href="/customer" class="list">     Customer Management</a>

<br>
<h3>SearchResults:</h3>
<table>
    <thead> 
        <th>ID</th>
        <th>First Name</th> 
        <th>Last Name</th> 
        <th>Phone Number</th>
    </thead> 
    <tbody>
        {{#each search}} 
        <tr> 
            <td>{{CustomerID}}</td>
            <td>{{CusFirst}}</td> 
            <td>{{CusLast}}</td> 
            <td>{{PhoneNum}}</td> 
        </tr> 
        {{/each}} 
    </tbody> 
</table> 
</html>